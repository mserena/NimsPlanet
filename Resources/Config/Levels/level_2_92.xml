        <goal type="2" value="2" initWeight="1" maxWeight="17" increaseWeight="1" optional="false"/>
        <goal type="1" value="16" optional="false"/>
        <goal type="3" figure="2" value="50" optional="false"/>
        <goal type="3" figure="5" value="50" optional="false"/>
        
        <!-- Score stars-->
        <stars scores="5000,10000,15000"/>
        
        <!-- Figures weight -->
        <figureWeight id="0" weight="3"/>
        <figureWeight id="1" weight="4"/>
        <figureWeight id="2" weight="3"/>
